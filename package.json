    "multer": "^1.4.4",
    "mysql2": "^2.3.0",
    "parse-multipart": "^1.0.4",
    "pizzip": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",    
    "source-map-support": "^0.5.20",
    "string-to-ms": "^1.1.2",
