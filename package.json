{
  "name": "serverless-template",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "npm t -- --watchAll --verbose",
    "test:cov": "npm t -- --maxWorkers=60% --coverage",
    "test:dev": "npm t -- --watch --runInBand --verbose",
    "test:staged": "npm t -- --passWithNoTests --findRelatedTests --listTests",
    "sql": "sequelize-cli",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/4-framework/server.ts"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@jest/types": "^27.1.1",
